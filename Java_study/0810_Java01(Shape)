package J0809_Shape;

public class Shape {
private double area;
	
	Shape() {
		area = 0;
	}
	
	Shape(double area) {
		this.area = area;
	}
	
	double getArea() {
		return area;
	}
	
	void setArea(double area) {
		this.area = area;
	}
	
	@Override
	public String toString() {
		return "넓이는 " + area + "입니다.";
	}

}
