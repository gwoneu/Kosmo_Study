<script>
	const pid = "[[${pid}]]";
	//alert(pid); pid 가 넘어오는지 확인 alert
	getPost();
	function getPost(){
		$.ajax({
			type:"get",
			url:"/posts/read.json",
			data: {pid},
			success:function(data){
				console.log(data);
			}
		});
	}
</script>