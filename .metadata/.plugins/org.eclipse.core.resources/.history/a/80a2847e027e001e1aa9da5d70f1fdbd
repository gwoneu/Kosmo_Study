<!-- 타임리스 사용 선언 -->
<html xmlns:th="http://www.thymeleaf.org">
<div class="my-5">
	<h1 class="text-center">Post List</h1>
	<table class="table">
		<tr th:each="post:${posts}"> <!--th(타임리프)에 있는 반복문 = each-->
			<td th:text="${post.pid}"></td>
			<td th:text="${post.title}"></td>
			<td th:text="${post.writer}"></td>
		</tr>
	</table>
</div>